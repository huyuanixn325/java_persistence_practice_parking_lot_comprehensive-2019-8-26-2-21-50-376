<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="tws.repository.ParkingLotMapper">

    <insert id="insertParkingLot">
        insert into `parking_lot` (`parkingLotID`,`capatity`,`availablePositionCount`,`employeeID`)
        values (#{parkingLot.parkingLotID},#{parkingLot.capatity},#{parkingLot.availablePositionCount},#{parkingLot.employeeID})
    </insert>
</mapper>
